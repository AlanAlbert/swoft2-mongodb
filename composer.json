{
    "name": "anhoder/swoft2-mongodb",
    "type": "library",
    "keywords": [
        "swoole",
        "swoft",
        "swoft-component"
    ],
    "description": "swoft2-mongodb component for swoft2 framework",
    "homepage": "https://github.com/alanalbert/swoft2-mongodb",
    "authors": [
        {
            "name": "AlanAlbert",
            "homepage": "https://alanalbert.github.io/"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "mongodb/mongodb": "^1.6",
        "ext-mongodb": "*",
        "swoft/framework": "^2.0",
        "swoft/db": "^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Anhoder\\Mongodb\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    }
}
